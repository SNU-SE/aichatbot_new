# Production Environment Configuration
# This file contains production-specific overrides

VITE_APP_ENVIRONMENT=production
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=warn
VITE_ENABLE_DEVTOOLS=false

# Production-specific limits (more restrictive)
VITE_MAX_FILES_PER_UPLOAD=5
VITE_MAX_SEARCH_RESULTS=20
VITE_CHAT_TIMEOUT=30000

# Production performance settings (optimized)
VITE_ENABLE_CACHING=true
VITE_CACHE_TTL=600000
VITE_LAZY_LOADING_ENABLED=true
VITE_VIRTUAL_SCROLLING_ENABLED=true

# Production analytics
VITE_ANALYTICS_ENABLED=true
VITE_PRIVACY_MODE=true

# Production security (strict)
VITE_RATE_LIMIT_ENABLED=true
VITE_MAX_REQUESTS_PER_MINUTE=30