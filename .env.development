# Development Environment Configuration
# This file contains development-specific overrides

VITE_APP_ENVIRONMENT=development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
VITE_ENABLE_DEVTOOLS=true

# Development-specific limits (more permissive)
VITE_MAX_FILES_PER_UPLOAD=20
VITE_MAX_SEARCH_RESULTS=50
VITE_CHAT_TIMEOUT=60000

# Development performance settings
VITE_ENABLE_CACHING=false
VITE_LAZY_LOADING_ENABLED=false

# Development analytics (disabled for privacy)
VITE_ANALYTICS_ENABLED=false

# Development security (less restrictive)
VITE_MAX_REQUESTS_PER_MINUTE=120